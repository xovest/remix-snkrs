{
  "name": "@mcansh/snkrs",
  "version": "1.0.0",
  "description": "show off your sneaker collection",
  "repository": "mcansh/snkrs",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "remix.config.js",
  "sideEffects": false,
  "scripts": {
    "dev": "pm2-dev pm2.config.js",
    "prebuild": "npm run clean",
    "build": "run-s build:*",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:icons": "node scripts/heroicon-symbols.mjs",
    "build:remix": "remix build",
    "generate": "prisma generate",
    "gen:types": "gen-env-types .env -o app/@types/global.d.ts -e .",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx,.mjs,.cjs,.cts,.mts .",
    "test": "jest --passWithNoTests",
    "type-check": "tsc",
    "validate": "run-p lint type-check test",
    "db:migrate": "prisma migrate dev --preview-feature",
    "prepare": "husky install && remix setup && metronome setup",
    "clean": "del .cache build public/build app/styles app/icons/{solid,outline}",
    "start": "remix-serve ./build",
    "deploy": "flyctl deploy --build-arg REMIX_TOKEN=$REMIX_TOKEN --strategy bluegreen"
  },
  "dependencies": {
    "@headlessui/react": "1.4.2",
    "@metronome-sh/node": "1.0.8",
    "@metronome-sh/react": "1.0.8",
    "@prisma/client": "3.5.0",
    "@remix-run/node": "1.0.5",
    "@remix-run/react": "1.0.5",
    "@remix-run/serve": "1.0.5",
    "accounting": "0.4.1",
    "cloudinary": "1.27.1",
    "cloudinary-build-url": "0.2.1",
    "clsx": "1.1.1",
    "cuid": "2.1.8",
    "date-fns": "2.26.0",
    "etag": "1.8.1",
    "express": "4.17.1",
    "fathom-client": "3.2.0",
    "framer-motion": "5.3.3",
    "ioredis": "4.28.1",
    "lodash.uniqby": "4.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-toast": "2.1.1",
    "react-number-format": "4.8.0",
    "remix": "1.0.5",
    "remix-seo": "0.0.3",
    "reserved-email-addresses-list": "0.0.9",
    "slugify": "1.6.3",
    "type-fest": "2.6.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@cld-apis/types": "0.1.3",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@mcansh/eslint-config": "3.2.0",
    "@remix-run/dev": "1.0.5",
    "@tailwindcss/aspect-ratio": "0.3.0",
    "@tailwindcss/forms": "0.4.0-alpha.1",
    "@testing-library/jest-dom": "5.15.1",
    "@testing-library/react": "12.1.2",
    "@types/accounting": "0.4.2",
    "@types/etag": "1.8.1",
    "@types/ioredis": "4.28.1",
    "@types/jest": "27.0.3",
    "@types/lodash.uniqby": "4.7.6",
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/yup": "0.29.13",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "autoprefixer": "10.4.0",
    "cssnano": "5.0.11",
    "del-cli": "4.0.1",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "gen-env-types": "1.3.0",
    "heroicons": "1.0.5",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "lint-staged": "12.1.2",
    "make-dir": "3.1.0",
    "npm-run-all": "4.1.5",
    "pm2": "5.1.2",
    "postcss": "8.4.1",
    "postcss-100vh-fix": "1.0.2",
    "postcss-cli": "9.0.2",
    "prettier": "2.5.0",
    "prisma": "3.5.0",
    "shell-quote": "1.7.3",
    "svgo": "2.8.0",
    "tailwindcss": "3.0.0-alpha.1",
    "ts-jest": "27.0.7",
    "typescript": "4.5.2"
  }
}
